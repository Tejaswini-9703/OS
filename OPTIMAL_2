#include <stdio.h>
int main()
{
    int n, p, f, z, avail, ans;
    printf("enter num of pages: ");
    scanf("%d", &n);
    int ref[n];
    printf("enter ref string: \n");
    for (int i = 0; i < n; i++)
    {
        scanf("%d", &ref[i]);
    }
    printf("enter num of frames: ");
    scanf("%d", &f);
    int frames[f], future[f], t = 0;
    for (int i = 0; i < f; i++)
    {
        frames[i] = -1;
        future[i] = -1;
    }
    printf("Ref str \t\tframes/pages    \thit/fault");
    for(int i=0; i<n;i++){
        int max=-1;
        printf("\n");
        printf("%d\t",ref[i]);
        avail=0;
        for(int j=0;j<f;j++){
            if (frames[j]==ref[i])
            {
                avail=1;
                for(int k=0;k<f;k++){
                    printf("\t%d",frames[k]);
                }
                printf("\tH");
                break;
            }
        }
        if(avail==0){
            for(int j=0;j<f;j++){
                if (frames[j]==-1)
                {
                    frames[j]=ref[i];
                    avail=1;
                    for(int k=0;k<f;k++){
                        printf("\t%d",frames[k]);
                    }
                    printf("\tF");
                    break;
                }  
            }          
        }
        if (avail==0)
        {
            for(int j=0;j<f;j++){
                future[j]=-1;
                for(int k=i+1;k<n;k++){
                    if(frames[j]==ref[k]){
                        future[j]=k;
                        break;
                    }
                }
                if(future[j]==-1){
                    ans=j;
                    break;
                }
                
                if(future[j]>max){
                    max=future[j];
                    ans=j;
                }
            }
            frames[ans]=ref[i];
            avail=1;
            for(int k=0;k<f;k++){
                printf("\t%d",frames[k]);
            }
            printf("\tF");

        }
        
    }
}
